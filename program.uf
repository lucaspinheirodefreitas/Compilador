programa
    declare numero a, b, c, d;
    declare texto t1, t2;

    leia(a);
    leia(b);
    leia(t1);

    a:=3+3.0*2+3-1*6.9/3+d;

    se (a == b) entao {
        escreva(a);
    } senao {
        escreva(b);
        escreva(t2);
    }

    enquanto (a >= b) {
        leia(c);
        escreva(b);
        leia(a);
        c:=3-1;
    }

fimprog;